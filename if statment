#include<stdio.h>
#include<stdlib.h>
int main()
{
  int m;/*m= mark*/
  printf("Enter the mark:");
  scanf("%d",&m);
  if(m>85&&m<=100)
      {
          printf("Grae A");
      }
   else if(m>70&&m<=84)
      {
          printf("Grade B");
      }
    else if(m>55&&m<=69)
      {
        printf("Grade C");
      }
    else if(m>40&&m<=54)
      {
        printf("Grade D");
      }
    if(m<40)
    {
        printf("Grade F");
    }
    return 0;
}
